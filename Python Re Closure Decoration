from re import match,sub

def wrapper(f):
    def fun(l):
        p = r"(.*){0,3}(\d{5})(\d{5})$"
        return f([sub(p, lambda match: f"+91 {match.group(2)} {match.group(3)}", num) 
               for num in l])
    return fun

@wrapper
def sort_phone(l):
    print(*sorted(l), sep='\n')

if __name__ == '__main__':
    l = [input() for _ in range(int(input()))]
    sort_phone(l) 


