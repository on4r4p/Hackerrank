class MyRegex {

    String pattern = "^(25[0-5]|2[0-4][0-9]|[0-1][0-9]{2}|\\d{1,2})\\."
            + "(25[0-5]|2[0-4][0-9]|[0-1][0-9]{2}|\\d{1,2})\\."
            + "(25[0-5]|2[0-4][0-9]|[0-1][0-9]{2}|\\d{1,2})\\."
            + "(25[0-5]|2[0-4][0-9]|[0-1][0-9]{2}|\\d{1,2})$";
}    
    
